# Example terraform.tfvars file
# Copy this to terraform.tfvars and fill in your values

aws_region        = "eu-west-2"
ami_id            = "ami-07eb36e50da2fcccd"  # Update with latest AMI for your region
instance_type     = "t3.micro"
key_name          = "user-service-key"  # Replace with your SSH key pair name
db_instance_class = "db.t3.micro"
db_name           = "userdb"
db_username       = "admin"
db_password       = "MySecurePass123!"  # Change this to a secure password